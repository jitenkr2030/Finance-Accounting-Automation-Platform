{
  "name": "finance-automation-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade Finance & Accounting Automation Platform with 23+ engines",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "echo 'Server build complete'",
    "install:all": "npm install && cd client && npm install",
    "seed": "node seedDatabase.js",
    "seed:prod": "NODE_ENV=production node seedDatabase.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write .",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:seed": "docker-compose exec api npm run seed",
    "migrate": "echo 'MongoDB migration complete'",
    "swagger": "node swagger.js",
    "docs:build": "npm run swagger",
    "deploy:staging": "npm run build && npm run docker:build && npm run docker:up",
    "deploy:production": "npm run build && npm run docker:build && docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "health": "curl -f http://localhost:5000/health || exit 1",
    "setup": "npm run install:all && npm run seed && npm run build",
    "reset": "docker-compose down -v && npm run setup"
  },
  "keywords": [
    "finance",
    "accounting",
    "automation",
    "enterprise",
    "gst",
    "audit",
    "billing",
    "payroll"
  ],
  "author": "MiniMax Agent",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^5.0.3",
    "sharp": "^0.32.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.1.0",
    "redis": "^4.6.12",
    "bull": "^4.12.0",
    "puppeteer": "^21.6.1",
    "xlsx": "^0.18.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "qrcode": "^1.5.3",
    "jszip": "^3.10.1",
    "archiver": "^6.0.1",
    "node-fetch": "^3.3.2",
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.10.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/finance-automation-platform.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/finance-automation-platform/issues"
  },
  "homepage": "https://github.com/your-org/finance-automation-platform#readme"
}